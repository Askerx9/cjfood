{
  "name": "00_starter-package",
  "version": "1.0.0",
  "description": "test de slider avec une displacement map",
  "main": "index.js",
  "scripts": {
    "dev": "parcel ./src/**/*.html",
    "build": "parcel build ./src/**/*.html ",
    "lint": "eslint ."
  },
  "author": "",
  "license": "ISC",
  "browserslist": "last 3 versions",
  "parcel-plugin-structurize": {
    "scripts": {
      "match": "*.{js,js.map}",
      "folder": "js"
    },
    "styles": {
      "match": "*.{css,css.map}",
      "folder": "css"
    },
    "assets": {
      "match": "*.{png,svg,jpg,jpg2,jpeg,gif,bmp,webm}",
      "folder": "assets"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.6.1",
    "eslint-config-airbnb": "^18.0.1",
    "glslify-bundle": "^5.1.1",
    "glslify-deps": "^1.3.1",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-eslint": "^1.0.7",
    "parcel-plugin-imagemin": "^3.0.0",
    "parcel-plugin-nuke-dist": "^1.0.0",
    "parcel-plugin-structurize": "^1.1.5",
    "postcss-harmonize": "^3.1.3",
    "postcss-modules": "^1.4.1",
    "posthtml-img-autosize": "^0.1.4",
    "posthtml-modules": "^0.4.2",
    "sass": "^1.22.4"
  },
  "dependencies": {
    "gsap": "^3.0.2",
    "parcel-transformer-google-fonts": "^0.1.3",
    "scrollmagic": "^2.0.7"
  }
}
